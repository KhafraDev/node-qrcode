{
  "name": "@khaf/qrcode",
  "version": "0.0.10",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "qrcode",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-linux-android",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl",
        "aarch64-pc-windows-msvc",
        "armv7-unknown-linux-gnueabihf",
        "x86_64-unknown-linux-musl",
        "x86_64-unknown-freebsd",
        "i686-pc-windows-msvc",
        "armv7-linux-androideabi"
      ]
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "2.11.4",
    "@types/node": "^17.0.31",
    "ava": "^4.0.1",
    "benny": "^3.7.1"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "ava",
    "version": "napi version"
  },
  "description": "Native bindings to create QR-codes in a variety of formats!",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KhafraDev/node-qrcode.git"
  },
  "keywords": [
    "qrcode",
    "qr"
  ],
  "author": "Khafra",
  "bugs": {
    "url": "https://github.com/KhafraDev/node-qrcode/issues"
  },
  "homepage": "https://github.com/KhafraDev/node-qrcode#readme"
}
